    };

    var ccCvcContent = $('<p>For Visa, Mastercard, and Discover (left), the 3 digits on the back of your card.</p>' +
        '<p>For American Express (right), the 4 digits on the front of your card.</p>' +
        '<div class="cvc-preview-container two-card"><div class="amex-cvc-preview"></div><div class="visa-mc-dis-cvc-preview"></div></div>');

    $('#cc-cvc-help').popover({
      trigger: 'hover',
      html: true,
      content: ccCvcContent,
      placement: 'top'
    });

    $('#payment-form').validator({
      custom: {
        ccNumber: function (e) {
          return !$.payment.validateCardNumber(ccNumber.val());
        },
        ccExpMonth: function (e) {
          return !$.payment.validateCardExpiry(ccExp.payment('cardExpiryVal'));
        },
        ccExpYear: function (e) {
          return !$.payment.validateCardExpiry(ccExp.payment('cardExpiryVal'));
        },
        ccCvc: function (e) {
          return !$.payment.validateCardCVC(ccCvc.val(), cardType);
        }
      },
      errors: {
        ccNumber: 'Enter 16 digits',
        ccExpMonth: 'Enter a valid month',
        ccExpYear: 'Enter a valid year',
        ccCvc: 'Enter a 3 or 4 digit security code'
      }
    }).on('submit', function (e) {
      if (e.isDefaultPrevented()) {
        return false;
      }
    });
  </script>
<script>
  $(document).ready(function() {
    $("#PrePayButton").addClass("disabled");
    $("#navigate-away").hide();
  });
  function showPrepaymentForm(showWarning){
    $("#payment-form").hide();
    $("#prepayment_form").show();
    $("#navigate-away").hide();
    if (showWarning) {
      $("#navigate-away").show();
    }
  }
  function showCreditCardForm(){
    $("#payment-form").show();
    $("#prepayment_form").hide();
    $("#navigate-away").hide();
  }
</script>
  <script>
   (function() {
     var selector = "#buy_vpn_signup_monthly",
         event = "click",
         piaAmazon = undefined;

     piaAmazon = new PIA.Amazon({
       clientID: "amzn1.application-oa2-client.a0d3838d77ef411fa9635e3e9dd39372",
       merchantID: "A3CV2ZGVYRWWGQ",
       buttonID: "amazon_button_monthly",
       returnURL: "/payments/amazon_checkout/U2FsdGVkX1_X3Lxvmyl5n0AHgTF8wOL4fy_8JaBCjDV6QwnMQsHZjsUDwomHeGJF,XiOM0fSMvvHeXZ6bRaHsucfUFyM",
       imgSrc: "https://www.privateinternetaccess.com/assets/btn_amazon-a1ef8d8e4fbce7d136122563e7b8cee6982d9e49a8d4511a2a7264e233dba5b5.png",
       jQuery: $
     });

     new PIA.LazyScript({
       src: 'https://static-na.payments-amazon.com/OffAmazonPayments/us/js/Widgets.js',
       loadOnEvent: event,
       onSelector: selector,
       onFirstLoad: piaAmazon.onFirstScriptLoad,
       onFirstAndSubsequentLoad: piaAmazon.bindToButton
     });
   })();
  </script>
