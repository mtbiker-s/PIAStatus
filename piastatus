  <script>
    function setupPayments(plan, price) {
      current_plan = plan;
      current_price = price;
      var plans = ["monthly","yearly","sixmonthly"];
      var processors = ['paypal', 'amazon', 'bitpay', 'cashu', 'okpay', 'ripple'];
      $.each(plans, function(_i, value) {
        $('#terms_' + value).hide();
        $('#prepayment_terms_' + value).hide();
        $.each(processors, function(_j, processor) {
          $('#' + processor + '_form_' + value).hide();
        });
        $('.paymentwall-payment-method').hide();
      });
      $.each(processors, function(_j, processor) {
        $('#terms_' + plan).show();
        $('#prepayment_terms_' + plan).show();
        $('#' + processor + '_form_' + plan).show();
        $('.paymentwall-payment-method-' + plan).show();
      });

      switch($('#prepayment_processor').val()) {
        case 'bitpay':
          $('#prepayment_form').attr('action', bitpay_links[plan]);
          break;
        case 'cashu':
          $('#prepayment_form').attr('action', cashu_links[plan]);
          break;
        case 'okpay':
          $('#prepayment_form').attr('action', okpay_links[plan]);
          break;
        case 'ripple':
          $('#prepayment_form').attr('action', ripple_links[plan]);
          break;
      }
      $("#navigate-away").hide();
    }

    function adjustSlidingContainer(buyButton, boxPosition) {
      var slidingContainer = $('#slidingCheckoutForm');

      if ($(window).width() < 1200) {
        slidingContainer.toggle().removeClass('slidingCheckout--center slidingCheckout--right slidingCheckout--left').addClass('slidingCheckout--center');
        slidingContainer.detach().insertAfter($(buyButton).closest('.price-table__item'));
      } else {
        slidingContainer.toggle().removeClass('slidingCheckout--center slidingCheckout--right slidingCheckout--left').addClass('slidingCheckout--' + boxPosition);
        slidingContainer.removeAttr('style');
      }
    }

    $('.slidingCheckoutCancel').click(function (e) {
      $('#slidingCheckoutForm').fadeOut();
    });
  </script>
  <script>
    $('#buy_vpn_signup_monthly').attr('href',null);
    $('#buy_vpn_signup_monthly').click(function (e) {
